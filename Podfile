# Uncomment the next line to define a global platform for your project
platform :ios, ’10.0’

target 'newsapp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for newsapp
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'SDWebImage'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'TwitterKit'
  
  pod 'RxSwift'
  pod 'RxDataSources'
  pod 'RxCocoa'

  pod 'SnapKit'
  
  target 'newsappTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'newsappUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
